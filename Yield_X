	)

	CreateButton("Back_Button", "Back", 1, UDim2.new(0.825, 0, 0, 25), UDim2.new(0.5, 0, 0.875, 0), AboutFrame, nil, 
		function()
			if AboutFrame.Position == UDim2.new(0.5, 0, AboutFrame.Position.Y.Scale, 0) then
				AboutFrame:TweenPosition(UDim2.new(-0.5, 0, AboutFrame.Position.Y.Scale, 0), EASING_DIRECTION, EASING_STYLE, 0.175, false, nil)
				ContentFrame:TweenPosition(UDim2.new(0.5, 0, ContentFrame.Position.Y.Scale, 0), EASING_DIRECTION, EASING_STYLE, 0.175, false, nil)
			end
		end
	)

	MainFrame:TweenSize(UDim2.new(0, 300, 0, 300), EASING_DIRECTION, EASING_STYLE, 0.25, false, nil)

	StarterGui:SetCore("SendNotification", {
		Title = "Yield X "..VERSION;
		Text = "Made by: TheEvilDeveloper\n[@DaEvilDev]\n Press <"..string.gsub(tostring(TOGGLE_BUTTON), "Enum.KeyCode.", "").."> to toggle GUI"; 
		Duration = 500;
		Button1 = "OK";
	})
	
	GUILoaded = true
end

UserInputService.InputBegan:Connect(function(Input)
	if Input.KeyCode == TOGGLE_BUTTON then
		if ToggleSound ~= nil then
			ToggleSound:Play()
		end

		GUIDebounce = true

		if GUI.Main_Frame.Position == UDim2.new(GUI.Main_Frame.Position.X.Scale, 0, -1, 0) then
			GUI.Main_Frame:TweenPosition(GUILocation, EASING_DIRECTION, EASING_STYLE, 0.25, false, nil)
		else
			GUILocation = GUI.Main_Frame.Position
			wait(0.05)
			GUI.Main_Frame:TweenPosition(UDim2.new(GUI.Main_Frame.Position.X.Scale, 0, -1, 0), EASING_DIRECTION, EASING_STYLE, 0.25, false, nil)
		end

		wait(0.5)

		GUIDebounce = false
	elseif Input.KeyCode == FlyKeybind and CanFly ~= false then
		FlyFunc()
	elseif Input.KeyCode == Enum.KeyCode.W then
		FlyKeys.W = true
		if Fly then
			FlyControl.F = FlySpeed
		end  
	elseif Input.KeyCode == Enum.KeyCode.S then
		FlyKeys.S = true
		if Fly then
			FlyControl.B = -FlySpeed
		end  
	elseif Input.KeyCode == Enum.KeyCode.A then
		FlyKeys.A = true
		if Fly then
			FlyControl.L = -FlySpeed
		end  
	elseif Input.KeyCode == Enum.KeyCode.D then
		FlyKeys.D = true
		if Fly then
			FlyControl.R = FlySpeed
		end    
	end
end)

UserInputService.InputEnded:Connect(function(Input)
	if Input.KeyCode == Enum.KeyCode.W then
		FlyKeys.W = false
		if Fly then
			FlyControl.F = 0
		end
	elseif Input.KeyCode == Enum.KeyCode.S then
		FlyKeys.S = false
		if Fly then
			FlyControl.B = 0
		end
	elseif Input.KeyCode == Enum.KeyCode.A then
		FlyKeys.A = false
		if Fly then
			FlyControl.L = 0
		end
	elseif Input.KeyCode == Enum.KeyCode.D then
		FlyKeys.D = false
		if Fly then
			FlyControl.R = 0
		end
	end
end)

RunService.Heartbeat:Connect(function()
	if GUILoaded ~= false then
		if GUI == nil then
			local GUI = GUIParent:FindFirstChild("TheEvilDeveloper:YX")
		end
		if GUI ~= nil then
			local Fly_Button = GetButton("Fly_Button")

			if Fly_Button then
				SetButton(Fly_Button, Fly)
				if CanFly == true then
					if Fly_Button.Button_Content.Text ~= "Fly <"..string.gsub(tostring(FlyKeybind), "Enum.KeyCode.", "")..">" then
						Fly_Button.Button_Content.Text = "Fly <"..string.gsub(tostring(FlyKeybind), "Enum.KeyCode.", "")..">"
					end
				end
			end

			local Fog_Button = GetButton("Fog_Button")

			if Fog_Button then
				SetButton(Fog_Button, Fog)
			end

			local ESP_Button = GetButton("ESP_Button")

			if ESP_Button then
				SetButton(ESP_Button, ESP)
			end

			local CollisionButton = GetButton("Collision_Button")

			if CollisionButton then
				SetButton(CollisionButton, Collision)
			end

			local Infinite_Jump_Button = GetButton("Infinite_Jump_Button")

			if Infinite_Jump_Button then
				SetButton(Infinite_Jump_Button, InfiniteJump)
			end

			local Infinite_Zoom_Button = GetButton("Infinite_Zoom_Button")

			if Infinite_Zoom_Button then
				SetButton(Infinite_Zoom_Button, InfiniteZoom)
			end
		
		end
		
		local Humanoid = Player.Character:FindFirstChildOfClass("Humanoid")

		if Humanoid == nil or Humanoid.Health <= 0 then
			Fly = false
			InfiniteJump = false
		end

		FogFunc()
		ESPFunc()
		CollisionFunc()
		InfiniteZoomFunc()
		InfiniteJumpFunc()
	end
end)
